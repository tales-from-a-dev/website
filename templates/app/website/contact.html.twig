{% block new %}
    <twig:Turbo:Frame id="contact-form-container">
        <twig:Form:ContactForm :form="form" />
    </twig:Turbo:Frame>
{% endblock %}

{% block update %}
    <twig:Turbo:Stream:Replace target="[data-slot='contact-form']">
        <twig:Form:ContactForm :form="form" />
    </twig:Turbo:Stream:Replace>
{% endblock %}
