{%- set classes = html_classes(
    'text-base leading-snug font-medium group-data-[size=sm]/card:text-sm',
    attributes.render('class')|default(''),
) -%}

{#- Template -#}

<div
    data-slot="card-title"
    class="{{ classes|tailwind_merge }}"
    {{ attributes }}
>
    {%- block content %}{% endblock -%}
</div>
