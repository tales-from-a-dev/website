{
    "controllers": {
      "@symfony/ux-autocomplete": {
        "autocomplete": {
          "enabled": true,
          "fetch": "eager",
          "autoimport": {
            "tom-select/dist/css/tom-select.default.css": true,
            "tom-select/dist/css/tom-select.bootstrap5.css": false
          }
        }
      },
      "@symfony/ux-live-component": {
        "live": {
          "enabled": true,
          "fetch": "eager",
          "autoimport": {
            "@symfony/ux-live-component/styles/live.css": true
          }
        }
      }
    },
    "entrypoints": []
}
