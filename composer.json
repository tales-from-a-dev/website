{
    "name": "tales-from-a-dev/website",
    "type": "project",
    "license": "MIT",
    "description": "Tales from a Dev website",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-xml": "*",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.18",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.5",
        "elao/enum": "^2.0",
        "league/commonmark": "^2.7",
        "omines/antispam-bundle": "^0.1.9",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpdoc-parser": "^1.13",
        "runtime/frankenphp-symfony": "*",
        "symfony/amazon-mailer": "7.4.*",
        "symfony/asset": "7.4.*",
        "symfony/asset-mapper": "7.4.*",
        "symfony/console": "7.4.*",
        "symfony/dotenv": "7.4.*",
        "symfony/expression-language": "7.4.*",
        "symfony/flex": "^2.10",
        "symfony/form": "7.4.*",
        "symfony/framework-bundle": "7.4.*",
        "symfony/free-mobile-notifier": "7.4.*",
        "symfony/http-client": "7.4.*",
        "symfony/mailer": "7.4.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/object-mapper": "7.4.*",
        "symfony/property-access": "7.4.*",
        "symfony/property-info": "7.4.*",
        "symfony/rate-limiter": "7.4.*",
        "symfony/runtime": "7.4.*",
        "symfony/security-bundle": "7.4.*",
        "symfony/serializer": "7.4.*",
        "symfony/stimulus-bundle": "^2.13",
        "symfony/string": "7.4.*",
        "symfony/translation": "7.4.*",
        "symfony/twig-bundle": "7.4.*",
        "symfony/ux-icons": "^2.23",
        "symfony/ux-turbo": "^2.23",
        "symfony/ux-twig-component": "^2.23",
        "symfony/validator": "7.4.*",
        "symfony/web-link": "7.4.*",
        "symfony/yaml": "7.4.*",
        "symfonycasts/tailwind-bundle": "^0.10",
        "tales-from-a-dev/twig-tailwind-extra": "^1.0",
        "twig/extra-bundle": "^3.22",
        "twig/html-extra": "^3.22",
        "twig/intl-extra": "^3.22",
        "twig/markdown-extra": "^3.22",
        "twig/string-extra": "^3.22",
        "twig/twig": "^3.22"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.4",
        "doctrine/doctrine-fixtures-bundle": "^4.0",
        "friendsofphp/php-cs-fixer": "^3.12",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^11.5",
        "pierstoval/smoke-testing": "^1.2",
        "rector/rector": "^2.2",
        "symfony/browser-kit": "7.4.*",
        "symfony/css-selector": "7.4.*",
        "symfony/debug-bundle": "7.4.*",
        "symfony/maker-bundle": "^1.65",
        "symfony/phpunit-bridge": "7.4.*",
        "symfony/stopwatch": "7.4.*",
        "symfony/web-profiler-bundle": "7.4.*",
        "vincentlanglet/twig-cs-fixer": "^3.11",
        "zenstruck/browser": "^1.9",
        "zenstruck/foundry": "^2.8"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Fixtures\\": "fixtures/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.4.*",
            "docker": true
        }
    }
}
